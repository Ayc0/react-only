{
  "extends": ["airbnb"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "no-underscore-dangle": "off",
    "no-confusing-arrow": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/destructuring-assignment": "off"
  }
}
