{
  "name": "preact-only",
  "version": "0.8.0",
  "source": "src/index.ts",
  "umd:main": "../lib/react-only-preact.umd.js",
  "module": "../lib/react-only-preact.es.js",
  "main": "../lib/react-only-preact.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn -s move:tmp && yarn -s build:microbundle && yarn -s remove:tmp",
    "move:tmp": "mkdir -p ./tmp && ncp ../src/* ./tmp & ncp ./src/* ./tmp",
    "remove:tmp": "rm -rf tmp",
    "build:microbundle": "../node_modules/.bin/microbundle --name \"react-only/preact\""
  },
  "peerDependencies": {
    "preact": "^8.3.1"
  },
  "devDependencies": {
    "ncp": "^2.0.0"
  }
}
