{
  "name": "react-only",
  "version": "1.0.3",
  "description": "üîç <Only /> displays some contents for a specific screen size",
  "repository": "git@github.com:Ayc0/react-only.git",
  "keywords": [
    "react",
    "css",
    "media-queries",
    "bootstrap",
    "responsive"
  ],
  "author": "Ayc0",
  "license": "MIT",
  "bugs": "https://github.com/Ayc0/react-only/issues",
  "homepage": "https://github.com/Ayc0/react-only#readme",
  "scripts": {
    "build": "node_modules/.bin/pack build",
    "deploy": "node_modules/.bin/pack publish"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "node_modules/**/*",
            "tests/**/*",
            "preact/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "pika-plugin-unpkg-field"
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.3.7",
    "@pika/plugin-build-web": "^0.4.0",
    "@pika/plugin-ts-standard-pkg": "^0.4.0",
    "@types/react": "^16.8.19",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "pika-plugin-unpkg-field": "^1.1.0",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
